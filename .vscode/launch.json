  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug g",
      "program": "${workspaceFolder}/g",
      "args": ["tests/flex_simple/simple.txt"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug zlex_parser",
      "program": "${workspaceFolder}/tests/zlex_parser/zig-out/bin/zlex_parser",
      "args": ["../rpcalc/scan.l"],
      "cwd": "${workspaceFolder}/tests/zlex_parser"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug zison",
      "program": "${workspaceFolder}/zig-out/bin/zison",
      "args": ["bison", "tests/reccalc/parser.y"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug zlex",
      "program": "${workspaceFolder}/zig-out/bin/zlex",
      "args": ["-t", "dump_generated_l", "tests/rpcalc/scan.l"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug zlex flex",
      "program": "${workspaceFolder}/zig-out/bin/zlex",
      "args": ["flex", "--help"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug main.out",
      "program": "${workspaceFolder}/src/flex_parser/main.out",
      "args": [],
      "cwd": "${workspaceFolder}/src/flex_parser"
    }
  ]
